{
  "expo": {
    "name": "Einbuergerungstest Trainer",
    "slug": "exam-trainer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/de-flag.png",
    "scheme": "einbuergerungstestde",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/de-flag.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.migcien.examtrainer"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/de-flag.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/de-flag.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": false,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "449d09fd-cee2-43ec-8a9b-c5f289c4b26a"
      }
    }
  }
}


